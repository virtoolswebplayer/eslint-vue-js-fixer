{
  "name": "eslint-vue-js-fixer",
  "version": "1.1.1",
  "description": "eslint vue fixer",
  "main": "lib/eslint-vue-js-fixer.js",
  "scripts": {
    "test": "eslint --ext .js,.vue test",
    "fix": "node bin/vuefix.js -d test -e a.js -s less -b",
    "self": "node bin/vuefix.js -d lib"
  },
  "bin": {
    "vuefix": "./bin/vuefix.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/virtoolswebplayer/eslint-vue-js-fixer.git"
  },
  "keywords": [
    "eslint",
    "vue",
    "fix"
  ],
  "author": "gaoletian@finupgroup.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/virtoolswebplayer/eslint-vue-js-fixer/issues"
  },
  "homepage": "https://github.com/virtoolswebplayer/eslint-vue-js-fixer#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "eslint": "^4.16.0",
    "parse5": "^4.0.0",
    "recursive-readdir": "2.1.0",
    "yargs": "^6.6.0",
    "js-beautify": "^1.7.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0"
  }
}
